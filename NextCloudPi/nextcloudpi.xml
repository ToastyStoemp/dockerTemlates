<?xml version="1.0" encoding="utf-8"?>
<Container>
    <Name>NextCloudPi</Name>
    <Description>No Description</Description>
    <Registry>https://hub.docker.com/r/ownyourbits/nextcloudpi-x86/</Registry>
    <Repository>ownyourbits/nextcloudpi-x86</Repository>
    <BindTime>true</BindTime>
    <Privileged>false</Privileged>
    <Environment>
      <Variable>
        <Name>PUID</Name>
        <Value>99</Value>
      </Variable>
      <Variable>
        <Name>PGID</Name>
        <Value>100</Value>
      </Variable>
    </Environment>
    <Networking>
      <Mode>bridge</Mode>
      <Publish>
        <Port>
          <HostPort>80</HostPort>
          <ContainerPort>80</ContainerPort>
        </Port>
        <Port>
          <HostPort>443</HostPort>
          <ContainerPort>443</ContainerPort>
        </Port>
        <Port>
          <HostPort>4443</HostPort>
          <ContainerPort>4443</ContainerPort>
        </Port>
      </Publish>
    </Networking>
    <Data>
        <Volume>
            <HostDir>/mnt/user/appdata/NextCloudPi/</HostDir>
            <ContainerDir>/data</ContainerDir>
            <Mode>rw</Mode>
        </Volume>
    </Data>
    <Version/>
    <WebUI>http://[IP]:[PORT:4443]/</WebUI>
    <Banner/>
    <Icon/>
    <ExtraParams/>
</Container>
